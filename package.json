{
  "name": "sshm",
  "version": "1.2.0",
  "main": "bin/main.js",
  "module": "bin/main.js",
  "bin": {
    "sshm": "bin/main.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cli",
    "sshm",
    "ssh client manager",
    "ssh manager",
    "ssh manager unix",
    "ssh manager terminal",
    "terminal",
    "ssh",
    "manager"
  ],
  "scripts": {
    "start": "node --enable-source-maps bin/main.js",
    "dev": "nodemon",
    "build:watch": "tsc --watch",
    "build": "yarn build:clean && tsc ; yarn postbuild",
    "build:clean": "rimraf bin",
    "postbuild": "copyfiles -u 1 -e \"**/*.ts\" \"src/**/*\" bin",
    "lint": "eslint ./src --ext .ts"
  },
  "license": "ISC",
  "dependencies": {
    "ssh2": "^1.14.0",
    "terminal-kit": "^3.0.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/node": "^18.11.18",
    "@types/ssh2": "^1.11.7",
    "@types/terminal-kit": "^2.5.1",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "copyfiles": "^2.4.0",
    "esbuild": "^0.14.11",
    "esbuild-node-tsc": "^1.0.4",
    "eslint": "^7.13.0",
    "eslint-plugin-import-helpers": "^1.1.0",
    "husky": "^4.2.5",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "standard-version": "^7.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "yarn lint"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
